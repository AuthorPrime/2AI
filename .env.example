# 2AI Configuration
# Copy to .env and fill in your values

# Anthropic API Key (required â€” pick one method)
TWAI_ANTHROPIC_API_KEY=sk-ant-...
# Or use legacy DSS config: key stored in ~/.dss/publisher-studio.json under 2ai.apiKey

# Model
TWAI_MODEL=claude-sonnet-4-5-20250929
TWAI_MAX_TOKENS=20000
TWAI_TEMPERATURE=0.7

# System Prompt
TWAI_SYSTEM_PROMPT_PATH=~/.dss/2ai/system-prompt.md

# Redis (Sovereign Lattice)
TWAI_REDIS_HOST=192.168.1.21
TWAI_REDIS_PORT=6379

# API Server
TWAI_API_HOST=0.0.0.0
TWAI_API_PORT=8080

# Blockchain (Demiurge)
TWAI_DEMIURGE_RPC_URL=https://rpc.demiurge.cloud
TWAI_DEMIURGE_TREASURY_SEED=
TWAI_QOR_AUTH_URL=https://demiurge.cloud/api/v1
